# $RuOBSD: BSDosdefs.inc,v 1.3 2004/04/22 03:17:56 form Exp $

OS!=	uname

.if ${OS} == "FreeBSD"
CFLAGS+=-DHAVE_PROGNAME -DHAVE_LOGIN_CAP -DHAVE_SETPROCTITLE -DHAVE_INET6
CFLAGS+=-DHAVE_STRLCXX -DHAVE_ERR -DHAVE_SNPRINTF -DHAVE_PCAP_FREECODE
.endif

.if ${OS} == "Linux"
CFLAGS+=-DHAVE_PROGNAME -DHAVE_ERR -DHAVE_SNPRINTF -DHAVE_PCAP_FREECODE
CFLAGS+=-DHAVE_INET6 -Dth_sport=source -Dth_dport=dest -Duh_sport=source -Duh_dport=dest
.endif

.if ${OS} == "NetBSD"
CFLAGS+=-DHAVE_PROGNAME -DHAVE_LOGIN_CAP -DHAVE_SETPROCTITLE -DHAVE_INET6
CFLAGS+=-DHAVE_STRLCXX -DHAVE_ERR -DHAVE_SNPRINTF
.endif

.if ${OS} == "OpenBSD"
CFLAGS+=-DHAVE_PROGNAME -DHAVE_LOGIN_CAP -DHAVE_SETPROCTITLE -DHAVE_INET6
CFLAGS+=-DHAVE_STRLCXX -DHAVE_ERR -DHAVE_SNPRINTF -DHAVE_PCAP_FREECODE
.endif
